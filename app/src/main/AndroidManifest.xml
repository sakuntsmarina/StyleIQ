<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/mod"
        android:label="@string/app_name"
        android:roundIcon="@drawable/mod"
        android:supportsRtl="true"
        android:theme="@style/Theme.Bingo"
        tools:targetApi="31">
        <activity
            android:name=".DiorQuiz"
            android:exported="false" />
        <activity
            android:name=".ChanelQuiz"
            android:exported="false" />
        <activity
            android:name=".ValentinoQuiz"
            android:exported="false" />
        <activity
            android:name=".VersaceQuiz"
            android:exported="false" />
        <activity
            android:name=".DiorActivity"
            android:exported="false" />
        <activity
            android:name=".ChanelActivity"
            android:exported="false" />
        <activity android:name=".CartierQuiz" />
        <activity
            android:name=".ValentinoActivity"
            android:exported="false" />
        <activity
            android:name=".CartierActivity"
            android:exported="false" />
        <activity
            android:name=".VersaceActivity"
            android:exported="false" />
        <activity
            android:name=".ProfileActivity"
            android:exported="false" /> <!-- RegisterActivity is set as the first activity to appear -->
        <activity
            android:name=".RegisterActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity> <!-- Other activities -->
        <activity
            android:name=".LoginActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true" />
    </application>

</manifest>